@article{guerranti2023on,
  title={On the Adversarial Robustness of Graph Contrastive Learning Methods},
  author={Guerranti, Filippo and Yi, Zinuo and Starovoit, Anna and Mazen Kamel, Rafiq and Geisler, Simon and Günnemann, Stephan},
  journal={NeurIPS 2023 Workshop: New Frontiers in Graph Learning},
  category={conference},
  abbr={NeurIPS GLFrontiers},
  bibtex_show={true},
  abstract={Contrastive learning (CL) has emerged as a powerful framework for learning representations of images and text in a self-supervised manner while enhancing model robustness against adversarial attacks. More recently, researchers have extended the principles of contrastive learning to graph-structured data, giving birth to the field of graph contrastive learning (GCL). However, whether GCL methods can deliver the same advantages in adversarial robustness as their counterparts in the image and text domains remains an open question. In this paper, we introduce a comprehensive  tailored to assess the robustness of GCL models. We subject these models to  adversarial attacks targeting the graph structure, specifically in the evasion scenario. We evaluate node and graph classification tasks using diverse real-world datasets and attack strategies. With our work, we aim to offer insights into the robustness of GCL methods and hope to open avenues for potential future research directions.},
  html={https://openreview.net/forum?id=wz6l6yLTv1},
  pdf={https://openreview.net/attachment?id=wz6l6yLTv1&name=pdf},
  selected={true},
}


@article{guerranti2021caregivermatcher,
  title={CaregiverMatcher: Graph Neural Networks for Connecting Caregivers of Rare Disease Patients},
  author={Guerranti, Filippo and Mannino, Mirco and Baccini, Federica and Bongini, Pietro and Pancino, Niccoló and Visibelli, Anna and Marziali, Sara},
  journal={Procedia Computer Science},
  volume={192},
  pages={1696--1704},
  year={2021},
  publisher={Elsevier},
  html={https://www.sciencedirect.com/science/article/pii/S1877050921016690},
  selected={true},
  category={conference},
  abbr={Procedia CS},
  bibtex_show={true},
  abstract={Rare diseases affect a growing number of individuals. One key problem for patients and their caregivers is the difficulty in reaching experts and associations competent on a particular disease. As a consequence, caregivers, often family members of the patient, learn much about the disease from their own experience. CaregiverMatcher is a proof of concept providing a smart solution to build a network of caregivers, linked by a matching mechanism based on graph neural networks. The caregivers and their experience with rare diseases are described by node features. Associations and care centers are invited to share their knowledge on the platform.},
