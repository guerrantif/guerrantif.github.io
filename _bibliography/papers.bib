@article{guerranti2023comparison,
  title={A Comparison of Machine Learning Approaches for Predicting Employee Attrition},
  author={Guerranti, Filippo and Dimitri, Giovanna Maria},
  journal={Applied Sciences},
  volume={13},
  number={1},
  pages={267},
  year={2023},
  publisher={MDPI},
  category={conference},
  abbr={MDPI Appl. Sci.},
  bibtex_show={true},
  abstract={Employee attrition is a major problem that causes many companies to incur in significant costs to find and hire new personnel. The use of machine learning and artificial intelligence methods to predict the likelihood of resignation of an employee, and the quitting causes, can provide HR departments with a valuable decision support system and, as a result, prevent a large waste of time and resources. In this paper, we propose a preliminary exploratory analysis of the application of machine learning methodologies for employee attrition prediction. We compared several classification models with the goal of finding the one that not only performs best, but is also well interpretable, in order to provide companies with the possibility of improving those aspects that have been shown to produce the quitting of their employees. Among the proposed methods, Logistic Regression performs the best, with an accuracy of 88% and an AUC-ROC of 85%.},
  html={https://www.mdpi.com/2076-3417/13/1/267}
}

@thesis{guerranti2022approximation,
  title={Approximation of Logical Reasoning using Neural Networks}, 
  author={Guerranti, Filippo}, 
  year={2022},
  school={Università degli Studi di Siena},
  selected={true},
  category={thesis},
  abbr={MSc Thesis},
  bibtex_show={true},
  abstract={Integrating reasoning and learning capabilities is one of the main challenges of artificial intelligence. Neural-symbolic approaches combine symbolic and subsymbolic methods to benefit from the potential of the two paradigms, while reducing their drawbacks. These methods have found numerous applications in relational domains where prior knowledge, usually in the form of a first-order logic theory, is used to infer new information about the relationships between entities in the domain. However, many neural-symbolic approaches still suffer from scalability issues and are not always able to handle the intrinsic uncertainty of the real-world. Relational Reasoning Networks (R2N) is an end-to-end neural-symbolic model that enables multi-hop subsymbolic reasoning by exploiting higher-level relational knowledge about the underlying logical atoms. However, the robust and consistent integration of logic semantics within the model is still an open subject of research. This thesis aims to analyse and devise new methods to address this problem. Empirical results show that R2N outperforms the baseline knowledge graph embedding methods on all three tasks of the Countries dataset. Moreover, injecting logic semantics on the output layer of the model slightly enhances the overall performance compared to the model without semantics and improves the model's interpretability.}
} 
